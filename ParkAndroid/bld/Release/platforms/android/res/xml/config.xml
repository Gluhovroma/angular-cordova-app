<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ParkAndroid" version="1.0.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <feature name="PushNotification">
        <param name="android-package" onload="true" value="com.pushwoosh.plugin.pushnotifications.PushNotifications" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>Парк Горького</name>
    <description>ParkGorkogoApplication</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Glukhov Roman</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.0" />
    <preference name="windows-phone-target-version" value="8.1" />
    <preference name="Orientation" value="portrait" />
    <vs:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
    <vs:plugin name="com.pushwoosh.plugins.pushwoosh" version="3.5.8" />
    <vs:platformSpecificValues />
</widget>
